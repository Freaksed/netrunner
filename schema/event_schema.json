{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Netrunner Event",
  "type": "object",
  "properties": {
    "deck": { "$ref": "deck_schema.json" },
    "title": { "type": "string" },
    "type": { "const": "Event" },
