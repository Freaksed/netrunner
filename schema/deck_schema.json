{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Netrunner Deck",
  "type": "object",
  "properties": {
    "set": { "type": "string" },
    "id": { "type": "string" },
    "alliance": {
      "type": "string",
      "enum": ["corp", "runner"]
    },
    "faction": {
      "type": "string",
      "enum": [
        "Anarch",
        "Criminal",
        "Shaper",
        "Weyland",
        "Haas-Bioroid",
        "Jinteki",
        "NBN"
      ]
    },
    "influence": { "type": "integer" },
    "limit": { "type": "integer" }
  }
}
